<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="admin.AdminImpl">
	<!-- 회원 목록 전부 가져오기 -->
	<select id="ViewUserList" resultType="user.UserDTO">
		SELECT id, name, regidate, email, enabled, authority, userType FROM users WHERE id != 'admin';
	</select>
</mapper>
